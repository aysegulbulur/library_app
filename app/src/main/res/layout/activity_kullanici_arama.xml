<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KullaniciAramaActivity">

    <include layout="@layout/actionbar_app"
        android:id="@+id/actionbarKullaniciArama"/>

    <SearchView
        android:id="@+id/KullaniciArama"
        android:layout_alignParentTop="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"/>

    <ListView
        android:id="@+id/KullaniciAramaListe"
        android:layout_below="@+id/KullaniciArama"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</RelativeLayout>